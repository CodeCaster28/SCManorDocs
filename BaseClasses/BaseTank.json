        },
        {
          "choiceValue": "3",
          "choiceDescription": ""
        },
        {
          "choiceValue": "4",
          "choiceDescription": ""
        }
      ]
    },
    {
      "keyName": "minRange",
      "keyDescription": "Minimum view distance to shoot target, used for automatic turrets only.",
      "keyChoices": []
    },
    {
      "keyName": "maxRange",
      "keyDescription": "Maximum view distance to shoot target, used for automatic turrets only. Values above 2048 doesn't work.",
      "keyChoices": []
    },
    {
      "keyName": "_minlight",
      "keyDescription": "Sets the minimum light level. Set this value if entity is too dark on compiled map. Also useful for things like computer screens or holograms. Default is 0, max 1.",
